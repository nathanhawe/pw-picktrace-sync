{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "System": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command":  "Warning"
      }
    },
      "WriteTo": [
        { "Name": "Console" },
        {
          "Name": "File",
          "Args": {
            "path": "logs/log.json",
            "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
            "retainedFileCountLimit": 30
          }
        }
      ]
    }
}